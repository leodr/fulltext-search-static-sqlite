// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
    provider = "sqlite"
    url      = "file:../public/database.db"
}

generator client {
    provider = "prisma-client-js"
}

model Product {
    id          String @id @default(cuid())
    name        String
    description String
}
